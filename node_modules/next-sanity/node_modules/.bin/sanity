#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/node_modules/.pnpm/sanity@3.99.0_@emotion+is-p_2dc8747c86d0a70e06ade18ef91d6a92/node_modules/sanity/bin/node_modules:/mnt/d/node_modules/.pnpm/sanity@3.99.0_@emotion+is-p_2dc8747c86d0a70e06ade18ef91d6a92/node_modules/sanity/node_modules:/mnt/d/node_modules/.pnpm/sanity@3.99.0_@emotion+is-p_2dc8747c86d0a70e06ade18ef91d6a92/node_modules:/mnt/d/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/node_modules/.pnpm/sanity@3.99.0_@emotion+is-p_2dc8747c86d0a70e06ade18ef91d6a92/node_modules/sanity/bin/node_modules:/mnt/d/node_modules/.pnpm/sanity@3.99.0_@emotion+is-p_2dc8747c86d0a70e06ade18ef91d6a92/node_modules/sanity/node_modules:/mnt/d/node_modules/.pnpm/sanity@3.99.0_@emotion+is-p_2dc8747c86d0a70e06ade18ef91d6a92/node_modules:/mnt/d/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sanity@3.99.0_@emotion+is-p_2dc8747c86d0a70e06ade18ef91d6a92/node_modules/sanity/bin/sanity" "$@"
else
  exec node  "$basedir/../../../../../sanity@3.99.0_@emotion+is-p_2dc8747c86d0a70e06ade18ef91d6a92/node_modules/sanity/bin/sanity" "$@"
fi
